{
    "agent_config": {
        "agent_name": "Minimal Test Agent",
        "agent_type": "other",
        "agent_welcome_message": "Hello! Say something and I'll respond.",
        "tasks": [
            {
                "task_type": "conversation",
                "toolchain": {
                    "execution": "parallel",
                    "pipelines": [
                        [
                            "transcriber",
                            "llm",
                            "synthesizer"
                        ]
                    ]
                },
                "tools_config": {
                    "input": {
                        "format": "wav",
                        "provider": "twilio"
                    },
                    "transcriber": {
                        "provider": "deepgram",
                        "model": "nova-2",
                        "language": "en",
                        "stream": false,
                        "encoding": "linear16"
                    },
                    "llm_agent": {
                        "agent_type": "simple_llm_agent",
                        "agent_flow_type": "streaming",
                        "llm_config": {
                            "provider": "openai",
                            "model": "gpt-3.5-turbo",
                            "temperature": 0.7,
                            "request_json": false,
                            "max_tokens": 50
                        }
                    },
                    "synthesizer": {
                        "provider": "elevenlabs",
                        "provider_config": {
                            "voice": "Rachel",
                            "voice_id": "21m00Tcm4TlvDq8ikWAM",
                            "model": "eleven_turbo_v2_5"
                        },
                        "stream": true,
                        "buffer_size": 100.0,
                        "audio_format": "wav"
                    },
                    "output": {
                        "format": "wav",
                        "provider": "twilio"
                    }
                },
                "task_config": {
                    "hangup_after_silence": 30.0,
                    "interruption_backoff_period": 100,
                    "backchanneling": false,
                    "optimize_latency": false
                }
            }
        ]
    },
    "agent_prompts": {
        "task_1": {
            "system_prompt": "You are a helpful assistant. Respond briefly to what the user says."
        }
    }
}
